{
  "expo": {
    "name": "childguard",
    "slug": "childguard",
    "scheme": "childguard",
    "version": "1.0.0",
    "platforms": ["ios", "android"],
    "ios": {
      "bundleIdentifier": "com.childguard.app"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.jeriel0312.childguard",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "childguard",
            "host": "*"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "icon": "./assets/img/icon.png",
    "splash": {
      "image": "./assets/img/newlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-image-picker"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "38d789c2-39e8-4101-8a2b-fa843f0198d3"
      }
    },
    "owner": "joshua311"
  }
}
