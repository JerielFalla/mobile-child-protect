{
  "expo": {
    "name": "childguard",
    "deepLinking": true,
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "icon": "./assets/img/icon.png",
    "splash": {
      "image": "./assets/img/newlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "childguard",
    "plugins": [
      "expo-router",
      "expo-build-properties",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-image-picker"
    ],
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.jeriel0312.childguard"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0c0aeacd-2572-4ccb-a382-ed298318a174"
      }
    }
  }
}
